{
  "public": true,
  "trailingSlash": true,
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/(faq|who|about)(/*)",
      "destination": "/#about"
    },
    {
      "source": "/(contact)(/*)",
      "destination": "/#contact"
    },
    {
      "source": "/(flickr|photography|photos|photog)(/*)",
      "destination": "https://www.flickr.com/people/imtom/"
    },
    {
      "source": "/(flickr|photography|photos|photog)/(map|photosets|photos)(/*)",
      "destination": "https://thomasr.dev/flickr-toys/"
    },
    {
      "source": "/flickr/map/stations(/*)",
      "destination": "/journey-logging"
    },
    {
      "source": "/humans:slug*",
      "destination": "https://github.com/itsmeimtom/thomasr.me/graphs/contributors"
    },
    {
      "source": "/(cv|resume)(/*)",
      "destination": "https://cv.thomasr.me"
    },
    {
      "source": "/projects/twitterbot(/*)",
      "destination": "/twitterbot"
    },
    {
      "source": "/(faq/tech|faq/devices|tech|devices)(/*)",
      "destination": "/uses"
    },
    {
      "source": "/homelab(/*)",
      "destination": "/lab"
    },
    {
      "source": "/(solihull|solcol)(/*)",
      "destination": "https://solcol.thomasr.me"
    },
    {
      "source": "/(notion-at)(/*)",
      "destination": "https://github.com/tomatsolihull/notion-assignment-tracker/blob/master/README.md"
    },
    {
      "source": "/(projects|project|unsub|gogo|33rpm|speedtest-map|zrpc|boris|supercutter|mettranslinksystem)(/*)",
      "destination": "https://github.com/itsmeimtom"
    },
    {
      "source": "/(project|thing)(s?)/(\\w*)(/*)",
      "destination": "https://github.com/itsmeimtom"
	}
  ],
  "rewrites": [],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Powered-By",
          "value": "hugs"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Powered-By",
          "value": "hugs"
        }
      ]
    }
  ]
}
